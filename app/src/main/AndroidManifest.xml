<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jovanovic.stefan.sqlitetutorial">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Review Buku"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.jovanovic.stefan.sqlitetutorial.UpdateActivity"
            android:parentActivityName="com.jovanovic.stefan.sqlitetutorial.MainActivity"/>
        <activity
            android:name="com.jovanovic.stefan.sqlitetutorial.AddActivity"
            android:label="Add Book"
            android:parentActivityName="com.jovanovic.stefan.sqlitetutorial.MainActivity" />
        <activity
            android:name="com.jovanovic.stefan.sqlitetutorial.MainActivity"
            android:label="Book Library">
        </activity>
        <activity
            android:name="com.jovanovic.stefan.sqlitetutorial.UsernameInputActivity"
            android:exported="true"
            android:label="Glenn's Library"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>
